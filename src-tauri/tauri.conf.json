{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "VorteKia",
  "version": "0.1.0",
  "identifier": "com.tauri-react-redis-template.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "VorteKia",
        "width": 1280,
        "height": 720,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/vortekia-logo.ico", "icons/vortekia-logo.png"]
  }
}
